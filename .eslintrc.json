{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/no-unresolved": "off",
        "import/newline-after-import": ["warn", { "count": 2 }],
        "no-multiple-empty-lines": ["error", { "max": 2 }]
    }
}
